    <isprint value="${pdict.subscribeForm.attributes}" encoding="off" />>
                                <div class="row">
                                    <div class="col-sm-5">
                                        <div class="input-group subscribe-group">
                                            <input type="text" id="email" class="form-control input-email" name="hpEmailSignUp" placeholder="Email" aria-label="${Resource.msg('placeholdertext.form.emailsignup', 'homePage', null)}"/>
                                            <span class="input-group-append">
                                                <button type="submit" data-href="${URLUtils.url('EmailSubscribe-Subscribe')}" class="btn subscribe-email subscribe-button">Subscribe</button>
                                            </span>
                                        </div>
                                    </div>
