<div class="menu-group" role="navigation">
    <ul class="nav navbar-nav pt-2" role="menu">
        <isif condition="${pdict.categories}">
            <p class="shop-back-link back-link bolder-fonts heading"> &lt Back </p>
            <h3 class="t-header-option-tag bolder-fonts heading"> SHOP </h3>
            <isloop items="${pdict.categories}" var="category">
                <isset name="cgid" value="${category.ID}" scope="page"/>
                <p class="t-header-option bolder-fonts heading"><a href="${URLUtils.url('Search-Show','cgid', cgid)}" id="${category.id}" class="t-header-option bolder-fonts heading" role="link" tabindex="0">${category.ID}</a></p>
            </isloop>
        </isif>
    </ul>
</div>
