<form action="${URLUtils.url('Order-Track')}" class="trackorder" method="POST" name="trackorder-form">
    <div class="b-card__body">
        <h2 class="form-title pt-2 pb-3">Sign up</h2>
        <p class="track-order-header-text form-descr grey-text text-center h-spacing">Donâ€™t have an account? Sign up
            now.</p>

        <button class="cont-btn mb-5 cont-btn-pink ttu" type="submit" value="Create Account"
            name="dwfrm_login_register">Continue</button>
        <input type="hidden" name="csrf_token" value="">
        <hr>
        <isif condition="${pdict.isif === true}">
            <h2 class="form-title pt-2 pb-3">Contact Support</h2>
            <p class="track-order-header-text form-descr grey-text text-center h-spacing">Problems signing in? Contact
