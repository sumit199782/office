                <description xml:lang="x-default">JSON object string containing configuration for widget theme and display customization.</description>
                <type>text</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>{}</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="subproApiBaseUrl">
