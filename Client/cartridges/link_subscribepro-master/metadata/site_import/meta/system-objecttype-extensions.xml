                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>Log in to place order with subscription</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="subproApplePayLoginMsg">
                <display-name xml:lang="x-default">Subpro Apple Pay Login Message</display-name>
                <description xml:lang="x-default">Message informing customer that he/she needs to be logged in as a registered customer to use Apple Pay with SubPro Subscription products</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>You need to be logged in as a registered customer to use Apple Pay with SubPro Subscription products</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="subproCreditCardRequirementMsg">
                <display-name xml:lang="x-default">Subpro Credit Card Requirement Message</display-name>
