{
  "name": "link_avalara",
  "version": "20.1.0",
  "description": "AvaTax for Salesforce B2C Commerce",
  "main": "",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "sgmf-scripts --test test/unit/**/*.js",
    "cover": "sgmf-scripts --cover",
    "test:integration": "sgmf-scripts --integration 'test/integration/**/*.js'",
    "test:acceptance": "./node_modules/.bin/codeceptjs run --verbose",
    "test:acceptance:launchReport": "./node_modules/.bin/allure serve test/acceptance/report",
    "compile:scss": "sgmf-scripts --compile css",
    "compile:js": "sgmf-scripts --compile js",
    "compile:fonts": "node bin/Makefile compileFonts",
    "build": "npm run compile:js;npm run compile:fonts;npm run compile:scss;",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "sgmf-scripts --lint css",
    "lint:js": "sgmf-scripts --lint js",
    "upload": "sgmf-scripts --upload",
    "uploadCartridge": "sgmf-scripts --uploadCartridge bm_avatax && sgmf-scripts --uploadCartridge int_avatax_sfra && sgmf-scripts --uploadCartridge int_avatax_svcclient && sgmf-scripts --uploadCartridge int_avatax",
    "watch": "sgmf-scripts --watch",
    "watch:static": "sgmf-scripts --watch static",
    "release": "node bin/Makefile release --"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalesforceCommerceCloud/link_avalara.git"
  },
  "author": "Shailesh Hatte <shailesh.hatte@avalara.com>",
  "license": "ISC",
  "homepage": "https://github.com/SalesforceCommerceCloud/link_avalara",
  "devDependencies": {
    "@tridnguyen/config": "^2.3.1",
    "@wdio/selenium-standalone-service": "^5.8.0",
    "allure-commandline": "^2.9.0",
    "chai": "^3.5.0",
    "chai-subset": "^1.5.0",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "cldr-data": "^32.0.1",
    "codeceptjs": "^2.1.0",
    "css-loader": "^0.28.11",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "globalize": "^1.1.1",
    "istanbul": "^0.4.4",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.15.0",
    "properties-parser": "^0.3.0",
    "proxyquire": "1.7.4",
    "request-promise": "^4.2.2",
    "sgmf-scripts": "^2.1.1",
    "shelljs": "^0.8.2",
    "should": "^13.2.3",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "cleave.js": "^1.0.2",
    "flag-icon-css": "^2.3.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.1",
    "npm": "^6.10.3"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "packageName": "",
  "babel": {
    "presets": [
      "env"
    ]
  }
}
