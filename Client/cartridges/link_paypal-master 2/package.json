    "qs": "^6.2.0",
    "rewire": "^4.0.1",
    "url": "^0.11.0",
    "nyc": "^15.1.0"
  },
  "optionalPeerDependencies": {
    "dw-api-mock": "git+ssh://git@github.com/SalesforceCommerceCloud/dw-api-mock.git"
  },
  "packageName": "int_paypal",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run huskeyLint",
      "pre-push": "npm run test"
    }
  }
}
