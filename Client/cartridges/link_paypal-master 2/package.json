  "dependencies": {
    "babel-plugin-rewire": "^1.2.0",
    "babel-register": "^6.26.0",
    "browserify": "^13.1.0",
    "es6-promise": "^4.2.8",
    "exports-loader": "^0.7.0",
    "imagesloaded": "^3.2.0",
    "imports-loader": "^0.8.0",
    "lodash": "^4.17.5",
