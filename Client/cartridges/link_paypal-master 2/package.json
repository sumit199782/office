{
  "name": "paypal",
  "version": "21.3.0",
  "description": "Paypal SFRA cartridge",
  "main": "index.js",
  "engines": {
    "node": ">=4.0 <12.0.0"
  },
  "scripts": {
    "uploadSfra": "sgmf-scripts --uploadCartridge int_paypal",
    "uploadSfraCartridge": "sgmf-scripts --uploadCartridge app_storefront_base && sgmf-scripts --uploadCartridge modules",
    "upload": "npm run uploadSfra && npm run uploadSfraCartridge",
    "compile:js": "sgmf-scripts --compile js",
    "startSelenium": "./node_modules/.bin/selenium-standalone start",
    "unit:coverageReport": "nyc --reporter=lcov --reporter=text-summary mocha test/unit/**/*.js",
    "unit:testReport": "mocha test/unit/**/*.js --reporter=mochawesome",
    "test": "sgmf-scripts --test test/unit/**/*.spec.js",
    "lint": "eslint --fix cartridges/int_paypal/**/*.js && eslint --fix cartridges/bm_paypal/**/*.js && eslint --fix cartridges/paypal_credit_financing_options/**/*.js && eslint --fix cartridges/paypal_credit_messaging/**/*.js && eslint --fix test/**/*.js && eslint --fix cartridges/app_testing/**/*.js",
    "lintTest": "eslint -c test/.eslintrc.json --fix test/unit/**/*.js",
    "huskeyLint": "eslint cartridges/int_paypal/**/*.js",
    "integration": "sgmf-scripts --integration 'test/integration/**/*.js'",
    "watch": "node watch.js",
    "uploadGC": "sgmf-scripts --uploadCartridge plugin_giftcertificate",
    "lint:isml": "./node_modules/.bin/isml-linter",
    "fix:isml": "./node_modules/.bin/isml-linter --autofix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SalesforceCommerceCloud/link_paypal.git"
  },
  "author": "Harry Potter",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-destructuring": "^7.8.8",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.9.2",
    "@tridnguyen/config": "^2.3.1",
    "@wdio/sauce-service": "^5.14.0",
    "@wdio/selenium-standalone-service": "^5.13.2",
    "allure-commandline": "^2.13.0",
    "appium": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "chai-subset": "^1.6.0",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "cldr-data": "^32.0.1",
    "codeceptjs": "^2.3.2",
    "codeceptjs-cucumber": "^2.0.10",
    "codeceptjs-saucehelper": "^1.4.0",
    "codeceptjs-saucelabs": "^2.0.6",
    "codeceptjs-shared": "^2.0.6",
    "css-loader": "^0.28.11",
    "debug": "^4.1.1",
    "deepmerge": "^3.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "globalize": "^1.4.2",
    "husky": "^3.1.0",
    "isml-linter": "^5.30.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "mocha": "^7.0.0",
    "mocha-junit-reporter": "^1.23.1",
    "mochawesome": "^6.2.2",
    "moment-timezone": "^0.5.26",
    "node-sass": "^4.12.0",
